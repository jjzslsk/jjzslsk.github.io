/* LESS Document */
@charset "UTF-8";
body {
  background: #efefef;
}
header {
  height: 40px;
  background: #fff;
  /* box-shadow: 0px 5px 5px #cccccc; */
  position: fixed;
  top: 0px;
  left: 0px;
  width: 100%;
  z-index: 100;
}
footer {
  height: 50px;
  background: #dbdbdb;
  box-shadow: 0px 5px 5px #cccccc;
  position: fixed;
  bottom: 0px;
  left: 0px;
  text-align: center;
  width: 100%;
  /* padding: 5px; */
}
footer a:link,
footer a:visited {
  display: inline-block;
  width: 23%;
  overflow: hidden;
}
.search-bar {
  display: inline-block;
  line-height: 30px;
  border: 1px solid #d9d9d9;
  border-radius: 3px;
  width: 80%;
  margin-top: 8px;
  vertical-align: middle;
}
a.btn-search:link,
a.btn-search:visited {
  display: inline-block;
  padding: 6px 0px;
  width: 18%;
  margin-top: 8px;
  margin-left: 2%;
  height: 34px;
  border: 1px solid #d9d9d9;
  border-radius: 3px;
  text-align: center;
  vertical-align: middle;
}
a.btn-search i {
  font-size: 125%;
  color: #666666;
}
a.back:link,
a.back:visited {
  display: block;
  text-align: left;
  font-size: 150%;
  color: #666666;
  padding-top: 8px;
}
h4.top-title {
  text-align: center;
  width: 100%;
  display: block;
  line-height: 40px;
  color: #666666;
  font-size: 100%;
}
a.todo:link,
a.todo:visited {
