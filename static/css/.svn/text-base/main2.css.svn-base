  font-size: 75%;
  color: rgba(255, 255, 255, 0.78);
  float: right;
}
.menuico {
  text-align: center;
  padding: 5px 0px 5px 0px;
}
.menuico i {
  font-size: 150%;
  color: #666666;
}
.menutx {
  text-align: center;
  font-size: 90%;
  color: #666666;
}

.mt-syl {
  padding-top: 0 !important;
}
.hea-syl{
  background: rgba(255, 255, 255, 0.30196078431372547);
}

.main-content {
